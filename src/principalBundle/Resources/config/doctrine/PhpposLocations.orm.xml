    </many-to-many>
    <many-to-many field="itemKit" target-entity="PhpposItemKits" inversed-by="location" fetch="LAZY">
      <join-table name="phppos_location_item_kits">
        <join-columns>
          <join-column name="location_id" referenced-column-name="location_id"/>
        </join-columns>
        <inverse-join-columns>
